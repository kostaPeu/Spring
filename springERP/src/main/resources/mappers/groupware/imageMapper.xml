<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="springERP.gw.mapper.imageMapper">
	<insert id="insertImage">
		insert into image_board_simple(img_board_id,emp_id,imgfile_name,imgfile_date) 
		values(image_board_seq,#{emp_id},#{imgfile_name},sysdate)
	
	</insert>
	
  
</mapper>


